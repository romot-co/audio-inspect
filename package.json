{
  "name": "audio-inspect",
  "version": "0.1.0",
  "description": "軽量かつ高機能なオーディオ解析ライブラリ",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./features": {
      "import": "./dist/features/index.js",
      "types": "./dist/features/index.d.ts"
    },
    "./features/time": {
      "import": "./dist/features/time.js",
      "types": "./dist/features/time.d.ts"
    },
    "./features/frequency": {
      "import": "./dist/features/frequency.js",
      "types": "./dist/features/frequency.d.ts"
    },
    "./core/fft-provider": {
      "import": "./dist/core/fft-provider.js",
      "types": "./dist/core/fft-provider.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run --coverage",
    "test:ci": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "lint": "eslint src/**/*.ts test/**/*.ts",
    "lint:fix": "eslint src/**/*.ts test/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts test/**/*.ts",
    "format:check": "prettier --check src/**/*.ts test/**/*.ts",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "audio",
    "analysis",
    "peaks",
    "typescript",
    "browser",
    "node"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitest/coverage-v8": "^3.1.4",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.3.0",
    "typescript": "^5.6.0",
    "vite": "^6.0.0",
    "vitest": "^3.1.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "webfft": "^1.0.3"
  }
}
